00061   if("ALM-EX3NV-VF" .EQ. ON) then goto 240
00070   c --------------------------
00080   c setpoint dinamico exaustor - em func dos TAE 
00090   if("CMD-A2P03PV3-QAP3PA05NV" .EQ. ON) then $LOC1=1 else $LOC1=0
00100   if("CMD-A2P06PV3-QAP3PA05NV" .EQ. ON) then $LOC2=1 else $LOC2=0
